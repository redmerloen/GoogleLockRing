<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Size of alarm alert outer ring. -->
    <dimen name="googlelockringview_outerring_diameter">270dip</dimen>

    <!-- Default target placement radius for GlowPadView. Should be 1/2 of outerring diameter. -->
    <dimen name="googlelockringview_target_placement_radius">135dip</dimen>

    <!-- Default glow radius for GlowPadView -->
    <dimen name="googlelockringview_glow_radius">75dip</dimen>

    <!-- Default distance beyond which GlowPadView snaps to the matching target -->
    <dimen name="googlelockringview_snap_margin">40dip</dimen>

    <!-- Default distance from each snap target that GlowPadView considers a "hit" -->
    <dimen name="googlelockringview_inner_radius">15dip</dimen>
    <dimen name="googlelockringview_margin_bottom">-48dip</dimen>
    <dimen name="googlelockringview_margin_right">0dip</dimen>
</resources>